{
    "version": 2,
    "routes": [
      {
        "src": "/user/(.*)",
        "dest": "https://nuxt-pwa-woad.vercel.app/user/$1"
      },
      {
        "src": "/_nuxt/(.*)",
        "headers": {
          "cache-control": "public, max-age=31536000, immutable"
        },
        "continue": true
      },
      {
        "src": "/(.*)",
        "dest": "https://groot-pwa-app-1.vercel.app/$1"
      }
    ],
    "cleanUrls": true,
    "trailingSlash": false
  }